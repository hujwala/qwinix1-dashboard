<div class="text-right dash-btn-group">
  <%= link_to 'Add New Dashboard', new_admin_dashboard_path, :remote => true, :class=>"btn btn-primary" %>
  <% if $dashboard_widget_github.present? || $dashboard_widget_github_url.present? || $dashboard_widget_code.present? || $dashboard_widget_jira.present? || $dashboard_widget_jira_project_key.present? %>

  <% if $dashboard_widget_github.present? %>
  <a href="/dashing/dashboards?access_token=<%= $dashboard_widget_github.access_token%>&organization_name=<%= $dashboard_widget_github.organization_name %>&repo_name=<%= $dashboard_widget_github.repo_name %>", class="btn btn-primary", target="_blank">View As Wallboard</a>
  <% elsif $dashboard_widget_github_url.present? %>
  <a href="/dashing/dashboards?github_url=<%= $dashboard_widget_github_url.github_url%>", class="btn btn-primary", target="_blank">View As Wallboard</a>
  <% elsif $dashboard_widget_code.present? %>
  <a href="/dashing/dashboards?code_api_token=<%= $dashboard_widget_code.code_api_token%>&code_repo_id=<%= $dashboard_widget_code.code_repo_id %>", class="btn btn-primary", target="_blank">View As Wallboard</a>
  <% elsif $dashboard_widget_jira.present? && $dashboard_widget_jira.jira_project_key.blank? %>
  <a href="/dashing/dashboards?rapidView=<%= $dashboard_widget_jira.jira_view_id %>", target="_blank">View As Wallboard</a>
  <% elsif $dashboard_widget_jira_project_key.present? %>
  <a href="/dashing/dashboards?rapidView=<%= $dashboard_widget_jira_project_key.jira_view_id %>&projectKey=<%= $dashboard_widget_jira_project_key.jira_project_key %>" , target="_blank">View As Wallboard</a>
  <% end %>
  <% end %>â€‹
</div>