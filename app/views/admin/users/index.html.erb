
<a data-toggle="modal nodashboard" data-target="#myModal"> 
<h3><button type="button"><%= link_to 'New user', new_admin_user_path,:remote => true %> <button> </h3></a>
<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
</div>
<% if @users.present? %>
<div class="row  col-sm-6 hidden-xs">
	<% @users.each do |user| %>
	<div class="col-sm-offset-1 col-sm-9">
		<div class="dashbords ">
			<div class="tiles">   
				<%= user.name %>
			</div>
		</div>
	</div>
	<% end %>
	<%= will_paginate @users, renderer: BootstrapPagination::Rails %>
</div>
<% else %>
<div class="nodashboard">No Users</div>
<% end %>